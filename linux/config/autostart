#!/usr/bin/bash

/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
nm-applet &
wired --run &
dropbox start -i &
xautolock -time 10 -locker 'u_autolocking lock' -detectsleep -corners ---- -notify 17 -notifier 'u_autolocking 15' &
flameshot &
1password --silent &
greenclip daemon &
u_eww &
sxhkd -c "$HOME/dotfiles/linux/config/sxhkdrc" &
kmonad "$HOME/dotfiles/linux/config/kmonad.kbd" &
u_polybar &
u_i3-workspaces &
